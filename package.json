{
  "name": "homan-io",
  "version": "0.0.1",
  "description": "Derek Homans website based off of Eleventy and EleventyOne",
  "author": "Derek Homan",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env ELEVENTY_ENV=dev eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=prod eleventy",
    "seed": "cross-env ELEVENTY_ENV=seed eleventy",
    "lighthouse": "node ./_lighthouse/lighthouse.js",
    "lighthouse-html": "node ./_lighthouse/lighthouseHtml.js",
    "optimize-images": "node ./optimize-images.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhoman/homan-io"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "dotenv": "^8.0.0",
    "glitch-canvas": "^1.1.5",
    "html-minifier": "^4.0.0",
    "jquery": "^3.4.0",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "postcss": "^7.0.17",
    "postcss-color-mix": "^1.1.0",
    "postcss-comment": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.1",
    "postcss-simple-vars": "^5.0.2",
    "precss": "^4.0.0",
    "uglify-js": "^3.6.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "chrome-launcher": "^0.13.0",
    "fast-glob": "^3.2.2",
    "fs-extra": "^8.1.0",
    "lighthouse": "^5.6.0",
    "path": "^0.12.7",
    "sharp": "^0.25.2"
  },
  "resolutions": {
    "terser": "3.14.*"
  }
}
